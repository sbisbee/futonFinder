{
  "name": "Futon Finder",
  "version": "1.0",
  "description": "Tries to find a running instance of CouchDB for any given page.",
  "homepage_url": "https://www.github.com/sbisbee/futonFinder",
  "content_scripts": [{
      "matches": ["http://*/*"],
      "js": ["context.js"]
  }],
  "permissions": [
    "http://*/*",
    "notifications",
    "tabs"
  ],
  "background_page": "background.html",
  "icons": { "48": "icon-48px.png" }
}
